{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },

    "ConnectionStrings": {
        "DefaultConnection": "Server=.;Database=FilmKirala;Trusted_Connection=True;TrustServerCertificate=True;"
    },
    "JwtSettings": {
        "Key": "BuranınÇokamaçokuzunolmasıgerektiğiiçin256dayetmiyo512hanelikuzunbirsecretkeyeldeedeceğimderkenhalilabimeselamolsunBursasporaBendenHakikiBaşarılar",
        "Issuer": "FilmKiralaApi",
        "Audience": "FilmKiralaClient",
        "ExpireMinutes": 60
    },
    "Serilog": {
        "Using": [ "Serilog.Sinks.Graylog" ],
        "MinimumLevel": "Information",
        "WriteTo": [
            {
                "Name": "Console"
            },
            {
                "Name": "Graylog",
                "Args": {
                    "hostnameOrAddress": "localhost",
                    "port": 12201,
                    "transportType": "Udp"
                }
            }
        ]
    },




    "AllowedHosts": "*"
}
